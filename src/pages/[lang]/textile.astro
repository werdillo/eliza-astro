---
import Layout from "../../layouts/Layout.astro";
import TextileItems from "../../components/TextileItems";

export function getStaticPaths() {
	return [
		{ params: { lang: "en" } },
		{ params: { lang: "lv" } },
		{ params: { lang: "ru" } },
	];
}
const { lang = 'lv' } = Astro.params;

---

<Layout>
	<TextileItems lang={lang} client:only />
</Layout>


---
import Layout from "../../../layouts/Layout.astro";
import Products from "../../../components/Products";

export function getStaticPaths() {
	return [
		{ params: { lang: "en" } },
		{ params: { lang: "lv" } },
		{ params: { lang: "ru" } },
	];
}
const { lang } = Astro.params;
---

<Layout>
	<Products lang={lang} type='bed' client:visible />
</Layout>
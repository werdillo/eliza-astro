---
import Layout from "../../layouts/Layout.astro";
import Contacts from "../../components/Contacts";

export function getStaticPaths() {
	return [
		{ params: { lang: "en" } },
		{ params: { lang: "lv" } },
		{ params: { lang: "ru" } },
	];
}
const { lang } = Astro.params;

---

<Layout>
	<Contacts lang={lang} client:load />
</Layout>


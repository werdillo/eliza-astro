---
import Layout from "../../../layouts/Layout.astro";
import BlogList from "../../../components/BlogList";

export function getStaticPaths() {
	return [
		{ params: { lang: "en" } },
		{ params: { lang: "lv" } },
		{ params: { lang: "ru" } },
	];
}
const { lang } = Astro.params;
    
---
<Layout>
    <BlogList lang={lang} type="projects" client:only/>
</Layout>